{
  "bundles": [
    {
      "src": "src/builder.js",
      "dest": "dist/builder.js",
      "format": "es",
      "babel": {
        "presets": [
          "es2016"
        ],
        "plugins": [
          "transform-async-generator-functions"
        ]
      }
    }, {
      "src": "src/builder.js",
      "dest": "dist/builder.js",
      "format": "cjs",
      "babel": {
        "presets": [
          ["es2015",
          {
            "modules": false
          }]
        ],
        "plugins": [
          "external-helpers",
          "transform-async-generator-functions"
        ]
      }
    }
  ],
  "sources": [
    {
      "src": ["src/workers/*.js"],
      "dest": "dist"
    }
  ]
}
